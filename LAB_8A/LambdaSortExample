import java.util.*;

public class LambdaSortExample {
    public static void main(String[] args) {
        List<String> stringList = Arrays.asList("banana", "apple", "cherry", "date");

        // Lambda expression to sort in descending order
        stringList.sort((s1, s2) -> s2.compareTo(s1));

        System.out.println("Sorted List in Descending Order: " + stringList);
    }
}
